{
  "name": "TAGML",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_chunk"
      }
    },
    "_chunk": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_tag"
        },
        {
          "type": "SYMBOL",
          "name": "end_tag"
        },
        {
          "type": "SYMBOL",
          "name": "milestone_tag"
        },
        {
          "type": "SYMBOL",
          "name": "text_variation"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "start_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_open_start_markup"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "start_layer_suffix"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\s+"
              },
              {
                "type": "SYMBOL",
                "name": "annotation"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_close_start_markup"
        }
      ]
    },
    "_open_start_markup": {
      "type": "STRING",
      "value": "["
    },
    "_close_start_markup": {
      "type": "STRING",
      "value": ">"
    },
    "end_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_open_end_markup"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "end_layer_suffix"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\s+"
              },
              {
                "type": "SYMBOL",
                "name": "annotation"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_close_end_markup"
        }
      ]
    },
    "_open_end_markup": {
      "type": "STRING",
      "value": "<"
    },
    "_close_end_markup": {
      "type": "STRING",
      "value": "]"
    },
    "milestone_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_open_start_markup"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "start_layer_suffix"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\s+"
              },
              {
                "type": "SYMBOL",
                "name": "annotation"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_close_end_markup"
        }
      ]
    },
    "start_layer_suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "+"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "name"
              }
            ]
          }
        }
      ]
    },
    "end_layer_suffix": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "name"
              }
            ]
          }
        }
      ]
    },
    "text_variation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_text_variation"
        },
        {
          "type": "SYMBOL",
          "name": "variant_text"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "text_variation_separator"
          }
        },
        {
          "type": "SYMBOL",
          "name": "end_text_variation"
        }
      ]
    },
    "begin_text_variation": {
      "type": "STRING",
      "value": "<|"
    },
    "text_variation_separator": {
      "type": "STRING",
      "value": "|"
    },
    "end_text_variation": {
      "type": "STRING",
      "value": "|>"
    },
    "variant_text": {
      "type": "SYMBOL",
      "name": "_chunk"
    },
    "name": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z0-9_]+"
      }
    },
    "text": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_regular_char"
            },
            {
              "type": "SYMBOL",
              "name": "_escaped_char"
            }
          ]
        }
      }
    },
    "_regular_char": {
      "type": "PATTERN",
      "value": "[^[<]"
    },
    "_escaped_char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\\\\"
        },
        {
          "type": "STRING",
          "value": "\\\""
        },
        {
          "type": "STRING",
          "value": "\\'"
        },
        {
          "type": "STRING",
          "value": "\\<"
        },
        {
          "type": "STRING",
          "value": "\\["
        },
        {
          "type": "STRING",
          "value": "\\!"
        },
        {
          "type": "STRING",
          "value": "\\|"
        }
      ]
    },
    "annotation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "basic_annotation"
        },
        {
          "type": "SYMBOL",
          "name": "ref_annotation"
        },
        {
          "type": "SYMBOL",
          "name": "identifying_annotation"
        }
      ]
    },
    "basic_annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "annotation_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_annotation_value"
        }
      ]
    },
    "ref_annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "annotation_name"
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "SYMBOL",
          "name": "ref_value"
        }
      ]
    },
    "identifying_annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "id_annotation"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "id_value"
        }
      ]
    },
    "annotation_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "_annotation_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_value"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_value"
        },
        {
          "type": "SYMBOL",
          "name": "number_value"
        },
        {
          "type": "SYMBOL",
          "name": "richtext_value"
        },
        {
          "type": "SYMBOL",
          "name": "list_value"
        },
        {
          "type": "SYMBOL",
          "name": "object_value"
        }
      ]
    },
    "ref_value": {
      "type": "SYMBOL",
      "name": "name"
    },
    "id_annotation": {
      "type": "STRING",
      "value": ":id"
    },
    "id_value": {
      "type": "SYMBOL",
      "name": "name"
    },
    "string_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "boolean_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[Tt][Rr][Uu][Ee]"
        },
        {
          "type": "PATTERN",
          "value": "[Ff][Aa][Ll][Ss][Ee]"
        }
      ]
    },
    "number_value": {
      "type": "PATTERN",
      "value": "[0-9]+(\\.[0-9]+)?"
    },
    "richtext_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[>"
        },
        {
          "type": "PATTERN",
          "value": "[^<]*"
        },
        {
          "type": "STRING",
          "value": "<]"
        }
      ]
    },
    "list_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "_annotation_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_annotation_value"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "object_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "annotation"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": []
}